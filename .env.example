# Bot Configuration
TOKEN=your_bot_token_here
CLIENT_ID=your_client_id_here
GUILD_ID=your_guild_id_here

# Optional: MongoDB Connection (if you want to use database)
MONGODB_URI=mongodb://localhost:27017/ticket-system

# Optional: External APIs
IMGUR_CLIENT_ID=your_imgur_client_id_here  # For image uploads
WEBHOOK_URL=your_webhook_url_here          # For external logging

# Optional: Development Settings
NODE_ENV=development                       # development or production
DEBUG=false                               # Enable debug logging
